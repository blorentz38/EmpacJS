{
	"type":"container",
	"id":"empacJsModule",
	"classList":["container", "module", "dark-mode", "has_bg", "shadow-box"],
	"attributes":["anim-drop-fade", "bg-pos", "custom-margin"],
	"properties":[
		{ "key": "--bg-img", "value": "url('/files/images/bg/abstract/empac-sound_waves-bg.jpg')" },
		{ "key": "--bg-pos-dsk", "value": "center center" },
		{ "key": "--bg-pos-mob", "value": "center center" },
		{ "key": "--padding-mult", "value": "2" }
