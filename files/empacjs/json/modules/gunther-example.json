{
	"type":"container",
	"id":"guntherExample",
	"classList":["container", "module", "light-mode", "has_bg", "row", "shadow-box"],
	"attributes":["anim-drop-fade", "bg-pos", "custom-margin"],
	"properties":[
		{ "key": "--bg-img", "value": "url('/files/images/bg/projects/gunther-example-bg.jpg')" },
		{ "key": "--bg-pos-dsk", "value": "center center" },
		{ "key": "--bg-pos-mob", "value": "center center" },
		{ "key":"--padding-mult", "value":"2"},
		{ "key":"--margin-mult", "value":"2"}
	],
	"elements":[ 
		{
			"type":"container",
			"classList":["container", "module", "row"],
			"attributes":["multi-dual", "custom-padding", {"key": "color-overlay", "value": "white"}],
			"properties":[
				{ "key": "--overlay-opacity", "value": "0.7" }
			],
			"elements":[
				{
					"type":"container",
					"classList":["content", "col", "align-center", "justify-center"],
					"attributes":["custom-padding", "order-adjust-mobile"],
					"properties":[
						{ "key":"--flex-order", "value":"1" }
					],
					"elements": [
						{
							"type": "image", 
							"alt": "gunther hammel logo",
							"size": "xlarge",
							"src": "/files/images/fg/projects/gh-logo_mono-blk.svg",
							"properties":[
								{ "key":"--size-mult", "value":"3" }
							]
						}
					]
				},
				{
					"type":"container",
					"classList":["content"],
					"attributes":["custom-padding", "order-adjust-mobile"],
					"properties":[
						{ "key": "--flex-order", "value":"2"}
					],
					"elements":[
						{
							"type":"headline",
							"tag":"h3",
							"size":"large",
							"content":"See the process in action."
						},
						{
							"type":"text",
							"tag":"p",
							"content":"We recently did a project for Gunther Hammel Photography where we put the DRIVE process to work."
						},
						{
							"type":"container",
							"classList":["cta-section", "row"],
							"attributes":["h-left"],
							"elements":[
								{
									"type":"link",
									"classList":["btn", "primary", "brand"],
									"content":"Read about the project",
									"aria_label":"Click to learn more about how DRIVE was used for Gunther's branding.",
									"href": "https://medium.com/@blorentzen/project-update-branding-for-gunther-hammel-photography-5fb8ccac7266",
									"target": "_blank"
								}
							]
						}
					]
				}
			]
		}
	]
}