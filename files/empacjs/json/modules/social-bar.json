{
	"type":"container",
	"id":"empacSocialBar",
	"classList":["container", "module", "dark-mode", "row", "align-center"],
	"attributes":["custom-margin", "multi-tri", "bg-color-brand"],
	"properties":[
		{ "key": "--padding-mult", "value": "1" },
		{ "key": "--margin-mult", "value": "1" }
	],
	"elements":[ 
		{
			"type":"container",
			"classList":["content", "row", "align-center", "justify-start"],
			"attributes":["custom-padding"],
			"elements":[
				{
					"type":"text",
					"tag":"span",
					"attributes":["cfs-size", "cfs-weight", "cfs-line_height"],
					"properties":[
						{"key":"--cf-size", "value":"1rem"},
						{"key":"--cf-weight", "value":"700"},
						{"key":"--cf-line_height", "value":"2em"}
					],
					"content":"Let's Connect"
				}
			]
		},
		{
			"type":"container",
			"classList":["content", "row", "align-center", "justify-center"],
			"attributes":["custom-padding", "social-bar"],
			"elements":[
				{
					"type":"link",
					"subtype":"social",
					"color":"white",
					"target":"_blank",
					"network":"facebook",
					"href":"https://facebook.com/emeraldpacific"
				},
				{
					"type":"link",
					"subtype":"social",
					"color":"white",
					"target":"_blank",
					"network":"twitter",
					"href":"https://twitter.com/emeraldpacific"
				},
				{
					"type":"link",
					"subtype":"social",
					"color":"white",
					"target":"_blank",
					"network":"instagram",
					"href":"https://instagram.com/emeraldpacific"
				},
				{
					"type":"link",
					"subtype":"social",
					"color":"white",
					"target":"_blank",
					"network":"linkedin",
					"href":"https://linkedin.com/company/emeraldpacific"
				}
			]
		},
		{
			"type":"container",
			"classList":["content", "row", "align-center", "justify-end"],
			"attributes":["custom-padding"],
			"elements":[
				{
					"type":"link",
					"aria-label":"Contact now",
					"classList":["btn", "tertiary"],
					"content":"Contact now",
					"href":"https://empac.co/contact"
				}
			]
		}
	]
}
